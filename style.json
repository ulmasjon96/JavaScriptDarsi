{
	"workbench.iconTheme": "material-icon-theme",
	"notebook.defaultFormatter": "esbenp.prettier-vscode",
	"redhat.telemetry.enabled": true,
	"liveServer.settings.donotVerifyTags": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"files.autoSave": "afterDelay",

	// cursar

	"workbench.colorCustomizations": {
		"editorCursor.foreground": "#00ff1e",
		"terminalCursor.foreground": "#00ff1e"
	},

	//comment color
	"editor.tokenColorCustomizations": {
		"comments": "#15ff00"
		// "strings": "#00eeffd1",
		// "types": "#3429d5",
		// "variables": "#2424e7",
		// "numbers": "#ffffff",
		// "keywords": "#ea00ff",
		// "functions": "#ffee00"
	},
	"workbench.editorAssociations": {
		"*.copilotmd": "vscode.markdown.preview.editor",
		"*.zip": "default"
	},
	"terminal.integrated.cursorWidth": 8,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.cursorStyleInactive": "line",
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.fontSize": 17,
	"editor.stickyScroll.enabled": false,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.cursorSmoothCaretAnimation": "on",
	"workbench.settings.applyToAllProfiles": ["liveshare.sharedTerminalWidth"],
	"cody.commandCodeLenses": true,
	"vs-kubernetes": {
		"vscode-kubernetes.helm-path-windows": "C:\\Users\\user\\.vs-kubernetes\\tools\\helm\\windows-amd64\\helm.exe",
		"vscode-kubernetes.kubectl-path-windows": "C:\\Users\\user\\.vs-kubernetes\\tools\\kubectl\\kubectl.exe",
		"vscode-kubernetes.minikube-path-windows": "C:\\Users\\user\\.vs-kubernetes\\tools\\minikube\\windows-amd64\\minikube.exe"
	},
	"editor.mouseWheelZoom": true,
	"editor.cursorWidth": 3,
	"workbench.editor.tabSizingFixedMaxWidth": 190,
	"scm.diffDecorationsGutterWidth": 5,
	"C_Cpp.default.compilerPath": "d:/dars/index.html",

	"editor.formatOnPaste": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"prettier.useTabs": true,
	"prettier.semi": false,
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true,
	"prettier.arrowParens": "avoid",
	"prettier.tabWidth": 4,

	"terminal.integrated.defaultProfile.windows": "Command Prompt",
	"terminal.integrated.fontSize": 16,
	"breadcrumbs.icons": false,
	"breadcrumbs.showKeys": false,
	"breadcrumbs.showFiles": false,
	"breadcrumbs.symbolPath": "off",
	"breadcrumbs.showArrays": false,
	"breadcrumbs.showBooleans": false,
	"breadcrumbs.showClasses": false,
	"breadcrumbs.showConstants": false,
	"breadcrumbs.showConstructors": false,
	"breadcrumbs.showEnumMembers": false,
	"breadcrumbs.showEvents": false,
	"breadcrumbs.showFields": false,
	"breadcrumbs.showEnums": false,
	"breadcrumbs.showFunctions": false,
	"breadcrumbs.showInterfaces": false,
	"breadcrumbs.showMethods": false,
	"breadcrumbs.showModules": false,
	"breadcrumbs.showNamespaces": false,
	"breadcrumbs.showNull": false,
	"breadcrumbs.showNumbers": false,
	"breadcrumbs.showObjects": false,
	"breadcrumbs.showOperators": false,
	"breadcrumbs.showPackages": false,
	"breadcrumbs.showProperties": false,
	"breadcrumbs.showStrings": false,
	"breadcrumbs.showStructs": false,
	"breadcrumbs.showTypeParameters": false,
	"breadcrumbs.showVariables": false,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.enabled": false,

	"editor.wordWrap": "on",
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"editor.tabSize": 2, // размер таба
	"editor.formatOnSave": true, // форматировать код при сохранении
	"explorer.compactFolders": false, // компактные папки
	"editor.smoothScrolling": true, // плавный скролл
	"workbench.list.smoothScrolling": true, // плавный скролл
	"terminal.integrated.smoothScrolling": true, // плавный скролл в терминале
	"editor.cursorBlinking": "expand", // мигающей курсор
	"terminal.integrated.cursorBlinking": true, // мигающей курсор
	"editor.glyphMargin": false,
	"editor.linkedEditing": true,
	"editor.padding.top": 15, // отступ сверху кода
	"zenMode.centerLayout": false,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"typescript": "typescriptreact"
	},

	"workbench.colorTheme": "Ayu Mirage Bordered",
	"http.proxySupport": "fallback",
	"security.workspace.trust.untrustedFiles": "open",
	"editor.fontFamily": "JetBrains Mono",
	"editor.fontLigatures": true,
	"editor.autoIndent": "full",
	"editor.lineHeight": 20,
	"editor.letterSpacing": 0.5,
	"editor.largeFileOptimizations": false,
	"tabnine.experimentalAutoImports": true,
	"editor.unicodeHighlight.nonBasicASCII": false,
	"editor.unicodeHighlight.invisibleCharacters": false
	// "editor.tokenColorCustomizations": {
	// "numbers": "#ffffff",
	// "functions": "#928f6b",
	// "types": "#00d9ff",
	// "keywords": "#ea00ff",
	// "strings": "#00eeffd1",
	// "variables": "#0088ff",
	// "comments": "#15ff00",
	// "types": "#00ff59"
	// }
}
